{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "dateFormat": "ddd MMM Do YYYY",
    "outputSourceFiles": false,
    "outputSourcePath": false,
    "systemName": "moron.js",
    "footer": "",
    "copyright": "moron.js Copyright © 2015-2015 Sami Koskimäki.",
    "navType": "vertical",
    "theme": "cyborg",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": true,
    "highlightTutorialCode": true,
    "syntaxTheme": "dark",
    "protocol": "html://"
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  }
}
